{
  "metricsets": {
    "_meta": {
      "version": "5.2.0"
    },
    "_all": {
      "norms": false
    },
    "dynamic_templates": [{
      "strings_as_keyword": {
        "match_mapping_type": "string",
        "mapping": {
          "ignore_above": 1024,
          "type": "keyword"
        }
      }
    }],
    "properties": {
      "@timestamp": {
        "type": "date"
      },
      "beat": {
        "properties": {
          "hostname": {
            "type": "keyword",
            "ignore_above": 1024
          },
          "name": {
            "type": "keyword",
            "ignore_above": 1024
          },
          "version": {
            "type": "keyword",
            "ignore_above": 1024
          }
        }
      },
      "meta": {
        "properties": {
          "cloud": {
            "properties": {
              "availability_zone": {
                "type": "keyword",
                "ignore_above": 1024
              },
              "instance_id": {
                "type": "keyword",
                "ignore_above": 1024
              },
              "machine_type": {
                "type": "keyword",
                "ignore_above": 1024
              },
              "project_id": {
                "type": "keyword",
                "ignore_above": 1024
              },
              "provider": {
                "type": "keyword",
                "ignore_above": 1024
              },
              "region": {
                "type": "keyword",
                "ignore_above": 1024
              }
            }
          }
        }
      },
      "metricset": {
        "properties": {
          "host": {
            "type": "keyword",
            "ignore_above": 1024
          },
          "module": {
            "type": "keyword",
            "ignore_above": 1024
          },
          "name": {
            "type": "keyword",
            "ignore_above": 1024
          },
          "namespace": {
            "type": "keyword",
            "ignore_above": 1024
          },
          "rtt": {
            "type": "long"
          }
        }
      },
      "metric": {
        "type": "keyword",
        "ignore_above": 1024
      },
      "long_value": {
        "type": "long",
        "doc_values": true,
        "index": "no"
      },
      "float_value": {
        "type": "scaled_float",
        "scaling_factor": 1000.0,
        "doc_values": true,
        "index": "no"
      },
      "tags": {
        "type": "keyword",
        "ignore_above": 1024
      },
      "type": {
        "type": "keyword",
        "ignore_above": 1024
      }
    }
  }
}